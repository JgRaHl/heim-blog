---
layout: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
  <% 
    sitemap.resources.reject do |resource|
      !resource.data.priority
    end.sort do |a,b|
      a.data.priority <=> b.data.priority
    end.reverse.each do |resource|
  %>
  <url>
    <loc>http://www.heim-projekt.com<%= resource.url %></loc>
    <priority><%= resource.data.priority %></priority>
  </url>
 <% end %>
</urlset>
